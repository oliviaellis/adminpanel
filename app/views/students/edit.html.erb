<h1> Edit <%= @student.first_name %> </h1>
<%= form_for ([@admin, @student]) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <%= f.label :education %>
  <%= f.select :education, Student.educations.keys.to_a %>

  <%= f.submit 'Submit'%>
<% end %>
