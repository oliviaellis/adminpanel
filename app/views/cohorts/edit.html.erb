<h1> Edit <%= @cohort.name %> </h1>
<%= form_for (@cohort) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :start %>
  <%= f.date_field :start %>
  <%= f.label :end %>
  <%= f.date_field :end %>

  <%= collection_check_boxes(:cohort, :student_ids, Student.all, :id, :name) %>
  
  <%= f.submit 'Submit'%>
<% end %>
